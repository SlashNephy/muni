{
  "name": "muni-pb",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/SlashNephy/muni"
  },
  "scripts": {
    "format": "concurrently -n format: -m 1 'yarn:format:*'",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "yarn lint:prettier --write",
    "lint": "concurrently -n lint: 'yarn:lint:*'",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check ."
  },
  "devDependencies": {
    "@slashnephy/eslint-config": "0.2.43",
    "@slashnephy/prettier-config": "0.1.12",
    "concurrently": "7.6.0",
    "eslint": "8.33.0",
    "prettier": "2.8.4",
    "typescript": "4.9.5"
  },
  "packageManager": "yarn@3.3.1",
  "eslintConfig": {
    "extends": [
      "@slashnephy/eslint-config"
    ]
  }
}
