{
  "name": "muni-frontend",
  "version": "0.0.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/SlashNephy/muni"
  },
  "scripts": {
    "build": "tsc && vite build",
    "build:storybook": "storybook build",
    "dev": "vite",
    "format": "concurrently -n format: -m 1 'yarn:format:*'",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "yarn lint:prettier --write",
    "lint": "concurrently -n lint: 'yarn:lint:*'",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006"
  },
  "dependencies": {
    "@emotion/react": "11.11.1",
    "@mantine/core": "5.10.5",
    "@mantine/hooks": "5.10.5",
    "@mantine/modals": "5.10.5",
    "@mantine/notifications": "5.10.5",
    "@mantine/nprogress": "5.10.5",
    "@mantine/spotlight": "5.10.5",
    "@marsidev/react-turnstile": "0.2.3",
    "@protobuf-ts/grpcweb-transport": "2.9.1",
    "@protobuf-ts/runtime": "2.9.1",
    "@tabler/icons-react": "2.30.0",
    "@tanstack/react-query": "4.32.6",
    "@u-wave/react-vimeo": "0.9.10",
    "@u-wave/react-youtube": "0.7.4",
    "framer-motion": "10.15.1",
    "i18next": "22.5.1",
    "i18next-browser-languagedetector": "7.1.0",
    "konva": "8.4.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "12.2.2",
    "react-konva": "18.2.10",
    "react-rnd": "10.4.1",
    "react-router-dom": "6.11.2",
    "react-screen-wake-lock": "3.0.1",
    "recoil": "0.7.7",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "11.11.0",
    "@slashnephy/eslint-config": "1.0.6",
    "@slashnephy/prettier-config": "0.1.27",
    "@storybook/addon-essentials": "7.0.27",
    "@storybook/addon-interactions": "7.0.27",
    "@storybook/addon-links": "7.0.27",
    "@storybook/blocks": "7.0.27",
    "@storybook/react": "7.0.27",
    "@storybook/react-vite": "7.0.27",
    "@storybook/testing-library": "0.2.0",
    "@types/node": "18.16.20",
    "@types/prop-types": "15",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "@vitejs/plugin-react-swc": "3.3.2",
    "concurrently": "8.2.0",
    "eslint": "8.46.0",
    "prettier": "2.8.8",
    "prop-types": "15.8.1",
    "storybook": "7.0.27",
    "typescript": "5.0.4",
    "vite": "4.3.9"
  },
  "packageManager": "yarn@3.6.1",
  "eslintConfig": {
    "extends": [
      "@slashnephy/eslint-config"
    ],
    "rules": {
      "import/extensions": "off"
    }
  }
}
