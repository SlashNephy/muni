{
  "name": "muni",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/SlashNephy/muni"
  },
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite",
    "format": "concurrently -n format: -m 1 'yarn:format:*'",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "yarn lint:prettier --write",
    "lint": "concurrently -n lint: 'yarn:lint:*'",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@emotion/react": "11.10.6",
    "@mantine/core": "5.10.4",
    "@mantine/hooks": "5.10.4",
    "@mantine/modals": "5.10.4",
    "@mantine/notifications": "5.10.4",
    "@mantine/nprogress": "5.10.4",
    "@mantine/spotlight": "5.10.4",
    "@protobuf-ts/grpcweb-transport": "2.8.2",
    "@protobuf-ts/runtime": "2.8.2",
    "@tabler/icons-react": "2.4.0",
    "konva": "8.4.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-konva": "18.2.5",
    "react-query": "3.39.3",
    "react-rnd": "10.4.1",
    "react-router-dom": "6.8.1",
    "react-screen-wake-lock": "3.0.1",
    "react-youtube": "10.1.0",
    "recoil": "0.7.6",
    "zod": "3.20.6"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "11.10.6",
    "@slashnephy/eslint-config": "0.2.43",
    "@slashnephy/prettier-config": "0.1.14",
    "@types/node": "18.14.1",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@vitejs/plugin-react": "3.1.0",
    "@vitejs/plugin-react-swc": "3.2.0",
    "concurrently": "7.6.0",
    "eslint": "8.35.0",
    "prettier": "2.8.4",
    "typescript": "4.9.5",
    "vite": "4.1.4"
  },
  "packageManager": "yarn@3.4.1",
  "eslintConfig": {
    "extends": [
      "@slashnephy/eslint-config"
    ],
    "overrides": [
      {
        "files": "vite.config.ts",
        "parserOptions": {
          "project": "./tsconfig.node.json"
        },
        "rules": {
          "import/no-default-export": "off"
        }
      },
      {
        "files": [
          "**/*.tsx"
        ],
        "rules": {
          "react/no-unknown-property": [
            "error",
            {
              "ignore": [
                "css"
              ]
            }
          ]
        }
      }
    ]
  }
}
